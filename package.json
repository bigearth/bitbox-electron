{
  "name": "BITBOX",
  "version": "0.3.5",
  "description": "A development framework for Bitcoin Cash",
  "main": "main.js",
  "productName": "BITBOX",
  "scripts": {
    "start": "electron .",
    "build": "webpack",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "create-installer-mac": "electron-installer-dmg ./release-builds/BITBOX-darwin-x64/BITBOX.app BITBOX --out=release-builds --overwrite --icon=assets/icons/mac/icon.icns",
    "package-windows": "electron-packager . bitbox --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/windows/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName='BITBOX'",
    "create-installer-windows": "node ./installers/windows/createinstaller.js",
    "test": "mocha --compilers js:babel-core/register"
  },
  "repository": "https://github.com/bigearth/bitbox-electron",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "Gabriel Cardona w/ EARTH",
  "license": "MIT",
  "devDependencies": {},
  "dependencies": {
    "axios": "^0.18.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "bchaddrjs": "^0.1.4",
    "bitbox-cli": "0.3.18",
    "bitcoinjs-lib": "^3.3.2",
    "bitcoinjs-message": "^2.0.0",
    "body-parser": "^1.18.2",
    "chai": "^4.1.2",
    "cors": "^2.8.4",
    "css-loader": "^0.28.9",
    "electron": "^1.7.12",
    "electron-packager": "^10.1.2",
    "electron-store": "^1.3.0",
    "express": "^4.16.2",
    "mocha": "^5.0.1",
    "moment": "^2.20.1",
    "node-sass": "^4.7.2",
    "qrcode.react": "^0.7.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-rangeslider": "^2.2.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-syntax-highlighter": "^7.0.0",
    "redux": "^3.7.2",
    "sass-loader": "^6.0.6",
    "satoshi-bitcoin": "^1.0.4",
    "style-loader": "^0.20.1",
    "underscore": "^1.8.3",
    "webpack": "^3.10.0"
  }
}
